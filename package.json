{
  "name": "phonetic-alphabet-converter",
  "version": "0.2.0",
  "description": "NATO phonetic alphabet converter.",
  "author": "Mark <mark@remarkablemark.org>",
  "main": "cjs/index.js",
  "scripts": {
    "build": "npm run build:cjs && npm run build:umd",
    "build:cjs": "tsc",
    "build:umd": "rollup --config",
    "clean": "rm -rf cjs umd",
    "coveralls": "cat coverage/lcov.info | coveralls",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "lint:fix": "npm run lint -- --fix",
    "prepublishOnly": "npm run lint && npm test && npm run clean && npm run build",
    "release": "standard-version --no-verify",
    "test": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "homepage": "https://remarkablemark.org/phonetic-alphabet-converter/",
  "repository": {
    "type": "git",
    "url": "https://github.com/remarkablemark/phonetic-alphabet-converter"
  },
  "bugs": {
    "url": "https://github.com/remarkablemark/phonetic-alphabet-converter"
  },
  "keywords": [
    "nato",
    "phonetic",
    "alphabet",
    "converter"
  ],
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@rollup/plugin-typescript": "^2.1.0",
    "@types/jest": "^24.9.1",
    "coveralls": "^3.0.9",
    "husky": "^4.0.10",
    "jest": "^25.1.0",
    "lint-staged": "^10.0.1",
    "prettier": "^1.19.1",
    "rollup": "^1.29.0",
    "standard-version": "^5",
    "ts-jest": "^25.0.0",
    "tslib": "^1.10.0",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.7.5"
  },
  "files": [
    "cjs/",
    "umd/"
  ],
  "license": "MIT"
}
